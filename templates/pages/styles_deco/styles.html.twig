{% extends 'base.html.twig' %}

{% block content %}

    <!--                                                 -Bandeau supérieur   / image de font 1894x359 px                                           -->

    <section class="page-header" style="background-image: url(assets/images/backgrounds/main-slider-11.jpg);">
        <div class="page-header-border"></div>
        <div class="page-header-border page-header-border-two"></div>
        <div class="page-header-border page-header-border-three"></div>
        <div class="page-header-border page-header-border-four"></div>
        <div class="page-header-border page-header-border-five"></div>
        <div class="page-header-border page-header-border-six"></div>
        <div class="container">
            <div class="page-header__inner">
                <h2>Styles de décoration</h2>
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a href="{{ path('app_home') }}">Home</a></li>
                    <li><span>/</span></li>
                    <li>Styles de décoration</li>
                </ul>
            </div>
        </div>
    </section>
    <!--                                                                -Bandeau supérieur fin                                                            -->

    <!--                                               STYLES DE DECORATION   / image de font 1894x359 px                                                  -->

    <section class="project-one portfolio-page">

        {% for style in style %}

            {% if style.image.first %}
                {% set image = vich_uploader_asset(style.image.first) %}
                {% set imageTitle = style.image.first.titre %}
            {% else %}
                {% set image = asset('assets/images/styles/charmant.png') %}
                {% set imageTitle = 'service image' %}
            {% endif %}

            <div class="container">
                <div class="row">
                    <div class="col-xl-4 col-lg-6 col-md-6">
                        <div class="project-one__single">
                            <div class="project-one__single">
                                <div class="project-one__img-box">
                                    <div class="project-one__img">
                                        <img src="{{ image }}" style="height: 370px; width: 100%; object-fit:cover"
                                             alt="{{ imageTitle }}">
                                        <div class="project-one__hover">
                                            <a href="{{ path('app_styles_details',{slug: style.slug}) }}"
                                               class="img-popup"><i class="fa fa-search-plus"></i></a>

                                            <div class="project-one__hover-content">
                                                <p class="project-one__hover-title">{{ style.titre1|default('style') }} </p>
                                                <h2 class="project-one__hover-name">
                                                    <a href="{{ path('app_styles_details',{slug: style.slug}) }}">
                                                        {{ style.titre2|default('styles scandinave...') }}</a>
                                                </h2>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
      {% endfor %}
    </section>

{% endblock %}





