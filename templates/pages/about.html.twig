{% extends 'base.html.twig' %}

{% block content %}

    <!--                                                 -Bandeau supérieur   / image de font 1894x359 px                                           -->
    <section class="page-header" style="background-image: url(assets/images/backgrounds/fond2.png);">
        <div class="page-header-border"></div>
        <div class="page-header-border page-header-border-two"></div>
        <div class="page-header-border page-header-border-three"></div>
        <div class="page-header-border page-header-border-four"></div>
        <div class="page-header-border page-header-border-five"></div>
        <div class="page-header-border page-header-border-six"></div>
        <div class="container">
            <div class="page-header__inner">
                <h2>About</h2>
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a href="{{ path('app_home') }}">Home</a></li>
                    <li><span>/</span></li>
                    <li>A propos</li>
                </ul>
            </div>
        </div>
    </section>
    <!--                                                                                  fin bandeau                                                                    -->


    <!--                                                                   SECTION 1 : SERVICES details  / image 672x713                                                   -->


    <section class="about-company">
        <div class="container">
            <div class="row">
                {% for contenu in about %}
                {% if contenu.type =="blogAboutConcept" %}

                <div class="col-xl-6">

                    {% if contenu.liste is defined %}
                        {% set clef = contenu.liste %}


                    <div class="about-company__img">

                        {% if contenu.Images[0] is defined %}
                            {% set image = contenu.Images[0] %}
                            <img src="{{ vich_uploader_asset(image) }}"
                                 alt="{{ image.Titre|default('image') }}">
                        {% endif %}
                     <div class="about-company__trusted-by">
                            <p>{{clef[0]|striptags}}</p>
                            <span>{{clef[1]|striptags}}</span>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6">
                    <div class="about-company__right">
                        <div class="section-title text-left">
                            <span class="section-title__tagline">{{contenu.titre1|striptags }}</span>
                            <h2 class="section-title__title">{{contenu.titre2|striptags }}</h2>
                        </div>
                        <ul class="about-company__points list-unstyled">
                            <li><span>/</span> {{contenu.texte1 |striptags}}}}</li>
                            <li><span>/</span> {{contenu.texte2 |striptags}}</li>
                        </ul>

                        <div class="about-company__progress">
                            <h4 class="about-company__progress-title">{{clef[2]|striptags}}</h4>
                            <div class="bar">
                                <div class="bar-inner count-bar counted" data-percent="{{clef[3]|striptags}}">
                                    <div class="count-text">{{clef[3]|striptags}}</div>
                                </div>
                            </div>
                        </div>
                        <div class="about-company__icon-and-text">
                            <div class="about-company__icon">
                                <span class="icon-blueprint"></span>
                            </div>
                            <div class="about-company__text-box">
                                <p class="about-company__text">{{clef[4]|striptags}}</p>
                            </div>


                        </div>
                        <p class="about-company__text-2">{{contenu.texte2 |striptags}} </p>
                        <a href="{{ path('app_portfolio')}}" class="about-company__btn thm-btn">Portfolio</a>
                    </div>
                </div>
                        {% endif %}
                {% endif %}
                {% endfor %}
            </div>
        </div>
    </section>
    <!--                                                                                  fin about me                                                                    -->


